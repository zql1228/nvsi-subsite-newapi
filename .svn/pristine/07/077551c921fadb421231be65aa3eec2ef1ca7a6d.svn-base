@font-face {
    font-family: "UniveConBol";
    src: url("UniveConBol.eot"); /* IE9 */
    src: url("UniveConBol.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
    
    url("UniveConBol.woff") format("woff"), /* chrome、firefox */
    url("UniveConBol.ttf") format("truetype"), /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
    
    url("UniveConBol.svg#UniveConBol") format("svg"); /* iOS 4.1- */
    font-style: normal;
    font-weight: normal;
}


