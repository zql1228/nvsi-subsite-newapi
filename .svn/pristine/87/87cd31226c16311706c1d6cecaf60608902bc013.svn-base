      getBasicInfo(){
          this.basicInfo=this.$store.getters.getBasic;
          this.countrySelect = this.basicInfo.albp0002 ? this.basicInfo.albp0002 : '156';
          this.nationSelect = this.basicInfo.albp0008 ? this.basicInfo.albp0008 : '01';
          this.politicsSelect = this.basicInfo.albp0007 ? this.basicInfo.albp0007.replace(/^\s+|\s+$/g,"") : '0';
          this.educationSelect = this.basicInfo.albp0017 ? this.basicInfo.albp0017.replace(/^\s+|\s+$/g,"") : '0';
          this.jobSelect = this.basicInfo.albp0018 ? this.basicInfo.albp0018 : '0';
          this.isInvite = this.basicInfo.albp0019 ? this.basicInfo.albp0019 : '1';
          this.isInviteOrg = this.basicInfo.albp0091 ? this.basicInfo.albp0091 : '1';
          this.serviceRange = this.basicInfo.albp0022 ? this.basicInfo.albp0022.split(',') : [];
          this.canServiceTime = this.basicInfo.albp0035 ? this.basicInfo.albp0035.split(',') : [];
          this.strength = this.basicInfo.albp0039 ? this.basicInfo.albp0039.split(',') : [];
          this.idcardV = this.basicInfo.albp2804? this.basicInfo.albp2804:0;//身份认证信息
          this.isQualification = this.basicInfo.albp2805?this.basicInfo.albp2805:'0';//是否取得涉税专业资格证书
          this.taxRevenue = this.basicInfo.albp2806?this.basicInfo.albp2806:'0';//涉税专业资格证书类别
          if(this.basicInfo.albp0027) {//设置照片
           //console.log(this.basicInfo.photo)
            $('#photo').html('<img style="width: 100%;height: 100%;" alt="" class="w" src="'+ this.context + this.basicInfo.photo +'" onerror="imgError(this)">');
          }
          if(this.basicInfo.albp0009) {//设置籍贯
            this.utilscommit.request('nvsi_getAreaFullName',{"areaid":this.basicInfo.albp0009},this.nativeBack);
          }
          if(this.basicInfo.albp0028) {//设置居住区域
            this.utilscommit.request('nvsi_getAreaFullName',{"areaid":this.basicInfo.albp0028},this.liveBack);
          }
           /*console.log(this.basicInfo.albp0011)
          if(this.basicInfo.albp0011 && this.basicInfo.albp0011.indexOf("*")==-1){//解密固定号码
            this.basicInfo.albp0011=this.utilscommit.getDecrypt(this.basicInfo.albp0011);
          } */
          //设置服务区域
          var nowareaid=this.$store.getters.getArea.areaid;
          var nowsitestye=0;//1=非本站
        if(this.basicInfo.albp0020 && nowareaid && this.basicInfo.albp0020.substring(0,2)!=nowareaid.substring(0,2) && !this.$store.getters.getArea.industrycode){//非本站志愿者\
            nowsitestye=1;
            this.serviceAreaName='';
          }else if(this.basicInfo.albp0020) {
            this.utilscommit.request('nvsi_getAreaFullName',{"areaid":this.basicInfo.albp0020},this.serviceBack);
          }
          if(this.basicInfo.albp0021) {//设置服务类型
            this.serviceType = this.basicInfo.albp0021.split(',');
            if(nowsitestye==1 && this.serviceType && this.serviceTypeList){//非本站点的志愿者，地方的服务类别需去除
                for (let i = 0; i < this.serviceTypeList.length; i++) {
                  if(this.serviceTypeList[i].albe0124!="0"){//地方的服务类别
                    for (let j = 0; j < this.serviceType.length; j++) {
                      if(this.serviceTypeList[i].albe0123==this.serviceType[j]){
                        this.serviceType.splice(j,1);
                      }
                    }
                  }
                }
            }
          }
          $('#nativeChoose_div').val('0').css('display','none');
          switch(this.countrySelect){
            case '156':
              $('#nativeChoose_div').val(this.basicInfo.albp0008).css('display','');
              break;
            case '344'://香港
              break;
            case '446'://澳门
              break;
            case '158'://台湾
              break;
            default:
              $('#politicsSelect').val('13').attr('disabled',true);
              $('#nationSelect').val('97').attr('disabled',true);
          }
      },
