      commit:function (serverData) {
        var data = {
          "id":this.userInfo.albp0055,
          "albp0002":this.countrySelect,//国籍
          "albp0007":this.politicsSelect,//政治面貌
          "albp0008":this.nationSelect,//民族
          "albp0009":this.basicInfo.albp0009,//籍贯
          "albp0010":this.basicInfo.albp0010,//手机
          "albp0011":this.basicInfo.albp0011,//固话
          "albp0012":this.basicInfo.albp0012,//QQ
          "albp0013":this.basicInfo.albp0013,//详细地址
          "albp0015":this.basicInfo.albp0015,//微信
          "albp0016":this.basicInfo.albp0016,//邮编
          "albp0017":this.educationSelect,//最高学历
          "albp0018":this.jobSelect,//从业状况
          "albp0019":this.isInvite,//受邀加入志愿项目
          "albp0020":this.basicInfo.albp0020,//服务区域
          "albp0021":this.serviceType.join(","),//服务类别
          "albp0022":this.serviceRange.join(","),//服务领域
          "albp0025":this.serviceAreaName,//服务区域名
          "albp0027":serverData?serverData:this.basicInfo.albp0027,//照片
          "albp0028":this.basicInfo.albp0028,//居住区域ID
          "albp0035":this.canServiceTime.join(","),//可服务时间
          "albp0039":this.strength.join(','),//个人特长
          "albp0047":this.liveAddress,//居住区域名
          "albp0081":this.basicInfo.albp0081,//邮箱
          "albp0091":this.isInviteOrg//受邀加入团体
        };
        if(this.serviceRange && this.serviceRange.length>0){
            let serviceRange=this.serviceRange.toString();
            if(serviceRange.indexOf("0013")!=-1){
              if(this.idcardV!=0)data.albp2804=this.idcardV;//身份认证信息
              if(this.isQualification!=0)data.albp2805=this.isQualification;//是否取得涉税专业资格证书
              if(this.taxRevenue!=0)data.albp2806=this.taxRevenue;//涉税专业资格证书类别
              if(this.basicInfo.albp2807)data.albp2807=this.basicInfo.albp2807;//涉税专业资格证书编号
            }
        }
        this.utilscommit.request("nvsi_modifyVolBasicInfo",data,this.modifyBack);
      },
      modifyBack:function (result) {
        if(result) {
          if(result.code==0){
            this.$swal('修改成功',{buttons:'确定'}).then(function (value) {
              Vuec.getUser();//刷新页面
            });
          } else {
            this.$swal('修改失败',{buttons:'确定'}).then(function (value) {
              Vuec.getUser();//刷新页面
            });
          } 
        }
      },
