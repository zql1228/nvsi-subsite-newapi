      imgCodeCheck:function () {
        this.imgCodeFlag = false;
        var el = $('#inputImgCode');
        var tip='';
        var type = 1;
        var code = el.val();
        if(!code) {
          tip='图文验证码不能为空';
          type = 2;
          this.formTip("imgCode",tip,type);
        }else{
          code = this.common.trimAll(code).toUpperCase();
          this.utilscommit.request('nvsi_getImgCodeCheck',{"imgcode":code,imgkey:this.imgkey},function (result) {
            if (result) {
              if (result.data) {
                Vues.imgCodeFlag = true;
              } else {
                tip = result.message;
                type = 2;
              }
            }
            Vues.formTip("imgCode",tip,type);
          });
        }
      },
      getPhotoCode:function () {
        this.status = 1;
        if (!this.bindPhoneFlag || !this.imgCodeFlag) {
          return null;
        }
        var code = this.common.trimAll($('#inputImgCode').val()).toUpperCase();
        var data = {
          "name":"NVSI",
          "phonenum":this.bindPhone,
          "userflag":"NVSI",
          "areaid":"0",
          "imgCode":code,
          imgkey:this.imgkey
        };
        this.utilscommit.request("nvsi_getMobileCode",data,this.getPhoneCodeBack);
      },
      getPhoneCodeBack:function (result) {
        if(result.data) {
          this.setPhotoTime();
        }
      },
      setPhotoTime:function () {
        if (this.countdown == 0) {
          $('#codebutton').removeAttr('disabled');
          this.codeButton = "获取验证码";
          this.countdown = 60;
          return;
        } else {
          $('#codebutton').attr('disabled',true);
          this.codeButton = this.countdown + "s";
          this.countdown--;
        }
        this.timer=setTimeout(function() {
          Vues.setPhotoTime();
        },1000);
      },
