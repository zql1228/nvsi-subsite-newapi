/* 首页固定窗口样式 */
.win-pop{
	width:320px;
	background:url(../img/pop_bg.png) no-repeat center center;
	position:fixed;
	box-sizing: border-box;
	padding:12px 24px 12px 24px;
	right:10px;
	bottom:0;
	border:1px solid #FFEAEA;
	transition: 0.5s;
	transform: translateY(100%);
	-ms-transform:translateY(100%);
	box-shadow: 0 3px 24px rgba(185, 185, 185,0.23);
	z-index: 101;
  }
  .win-pop.show{
	transform: translateY(0);
	-ms-transform:translateY(0);
	bottom:10px;
  }
  .win-pop-close{
	position: absolute;
	right:10px;
	top:10px;
	width:16px;
	height:16px;
	background:url(../img/close.png) no-repeat center center;
	cursor: pointer;
  }
  .win-pop p{
	font-size:14px;
	color:#333333;
	line-height:1.8;
	margin:0 0 5px 0;
  }
  .win-pop p span{
	display:inline-block;
	text-align: justify;
	text-align-last: justify;
  }
  .win-pop h2{
	font-size:20px;
	font-weight: bold;
	text-align:center;
	padding:0 30px;
	margin:0 0 14px 0;
	line-height:1.6;
  }


