body .bannerimg {
	background: url(../img/banner.png) no-repeat center top;
}

.mid-layout {
	width: 1200px;
	margin: 0 auto;
}

.header {
	height: 30px;
	background: #333;
	font-size: 12px;
	color: #fff;
}

.icon-link {
	color: inherit;
	text-decoration: none;
	margin-right: 20px;
}

.icon-link img {
	vertical-align: middle;
}

.icon-link span {
	display: inline-block;
	vertical-align: middle;
}

.label-link {
	color: inherit;
	text-decoration: none;
	transition: 0.3s;
}

.label-link:hover {
	color: #ccc;
}

.label-link:after {
	content: "|";
	font-size: 10px;
	margin-left: 10px;
	margin-right: 6px;
}

.label-link:last-child:after {
	display: none;
}

.banner {
	height: 280px;
}

.banner .logo {
	width: 460px;
	font-size: 0;
	height: 88px;
	background: url(../img/logo1.png) no-repeat left center;
	position: relative;
}

.banner .logo-area {
	display: block;
	font-size: 18px;
	color: #fff;
	line-height: 31px;
	padding: 0 15px 0 0;
	border-radius: 0px 6px 6px 0;
	position: absolute;
	right: 0;
	bottom: 10px;
	background: #E60012;
}

.banner .logo-area:before {
	content: "";
	display: inline-block;
	width: 22px;
	height: 31px;
	background: url("../img/head.png") no-repeat center center;
	position: absolute;
	left: -22px;
	top: 0;
}

.banner .search {
	background: #fff;
	display: inline-block;
	border-radius: 5px;
	padding: 0 0 0 10px;
}

.banner .search select {
	padding: 5px 30px 5px 8px;
	border: none;
	outline: none;
	background: none;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	background: url(../img/down.png) no-repeat right center;
	background-size: 14px;
}

.banner .search select::-ms-expand {
	display: none;
}

.banner .search input {
	font-size: 14px;
	color: #333;
	padding: 8px;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	border: none;
	outline: none;
	width: 200px;
}

.banner .search button {
	background: #e60012;
	height: 40px;
	line-height: 40px;
	border: 1px solid #e60012;
	color: #fff;
	padding: 0 20px;
	border-radius: 0 5px 5px 0;
	outline: none;
	font-size: 14px;
	cursor: pointer;
}

.nav {
	display: table;
	width: 100%;
	table-layout: fixed;
	border-bottom: 1px solid #999;
	height: 50px;
	margin-top: -25px;
}

.nav a {
	display: table-cell;
	vertical-align: middle;
	text-decoration: none;
	color: #333;
	text-align: center;
	position: relative;
}

.nav a.active {
	color: #e60012;
	font-weight: bold;
}

.nav a.active:before {
	content: '';
	display: block;
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 4px;
	background: #e60012;
}

.large-title {
	position: relative;
	padding-left: 100px;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	margin-top: 30px;
}

.large-title h2 {
	font-size: 40px;
	color: #E12929;
	margin: 0;
	padding-top: 30px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.large-title .type {
	position: absolute;
	left: 0;
	top: 0;
}

.large-title .type h3 {
	background: #E12929;
	font-size: 30px;
	font-weight: normal;
	color: #fff;
	margin: 0;
	font-style: italic;
	padding: 0 10px;
}

.large-title .type p {
	color: #E12929;
	font-size: 30px;
	margin: 0;
	font-weight: bold;
	font-style: italic;
	padding: 0 10px;
}

.large-title:before {
	content: "";
	display: block;
	height: 10px;
	background: url(../img/t.png) no-repeat center right;
	position: absolute;
	right: 0;
	top: 10px;
	width: 100%;
}

.title {
	color: #666;
	position: relative;
	margin-bottom: 20px;
}

.title img {
	vertical-align: middle;
	margin-right: 10px;
}

.title .label {
	font-size: 0;
	display: inline-block;
	vertical-align: middle;
}

.title .label span {
	display: inline-block;
	font-size: 24px;
}

.title .label span:before {
	content: "/";
	margin: 0 10px;
}

.title .label span:nth-child(1)::before {
	display: none;
}

.title p {
	font-size: 14px;
	color: #221815;
	display: inline-block;
	vertical-align: middle;
	margin-left: 10px;
	position: relative;
	top: 6px;
}

.title .more {
	font-size: 12px;
	background: #e60012;
	color: #fff;
	border: 1px solid #e60012;
	text-decoration: none;
	padding: 2px 8px;
	border-radius: 20px;
	position: absolute;
	right: 0;
	top: 50%;
	margin-top: -9px;
}
.title .more:hover{background:#fff;color: #e60012;}

.list {
	font-size: 16px;
	padding: 0;
	list-style: none;
	color: #333;
	margin: 0;
}

.list.small li {
	padding: 6px 0;
}

.list li {
	padding: 10px 0;
}

.list li .col:first-child {
	width: 24px;
}

.list li .col:last-child {
	width: 120px;
	text-align: right;
}

.list li .col .num {
	display: inline-block;
	width: 14px;
	height: 14px;
	line-height: 14px;
	text-align: center;
	background: #666;
	border-radius: 50%;
	color: #fff;
	font-size: 12px;
	vertical-align: middle;
}

.list li .col .label {
	font-size: 16px;
	color: inherit;
	text-decoration: none;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	display: inline-block;
	width: 100%;
	vertical-align: middle;
}

.list li .col .label:hover {
	color: #E12929;
}

.list li .col .label .dot {
	display: inline-block;
	width: 4px;
	height: 4px;
	background: #E12929;
	border-radius: 50%;
	vertical-align: middle;
	margin-right: 10px;
}

.list li .col .time {
	color: #808080;
}

.list li:nth-child(1) .num {
	background: #E12929;
}

.list li:nth-child(2) .num {
	background: #E12929;
}

.list li:nth-child(3) .num {
	background: #E12929;
}

.text {
	font-size: 16px;
	color: #333;
	margin: 10px 0;
}

.text a {
	color: #E12929;
	text-decoration: none;
}

.label-title h2 {
	font-size: 18px;
	color: #E12929;
	display: inline-block;
	vertical-align: middle;
	margin: 0;
}

.label-title .tag {
	background: #E12929;
	font-size: 13px;
	font-style: italic;
	color: #fff;
	display: inline-block;
	vertical-align: middle;
	padding: 1px 5px;
	margin-right: 5px;
}

.box {
	border: 1px solid #DCDCDC;
	padding: 6px;
	margin-bottom: 10px;
	cursor: pointer;
	transition: 0.3s;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}

.box:hover {
	border-color: #e60012;
}

.box .col:first-child {
	width: 35%;
}

.box:last-child {
	margin: 0;
}

.box h2 {
	font-size: 16px;
	margin: 0;
}

.box p {
	font-size: 12px;
	color: #666;
	margin: 0;
}

.center {
	font-size: 14px;
	background: url(../img/center.png) no-repeat center center;
	background-size: 100%;
	color: #fff;
	padding: 10px;
}

.center p {
	font-size: 18px;
	margin: 0;
}

.center h2 {
	font-size: 30px;
	font-weight: bold;
	margin: 0;
}

.center h3 {
	height: 75px;
	line-height: 75px;
	position: relative;
	text-align: center;
	margin: 0;
}

.center h3:after {
	content: "";
	display: block;
	border-width: 10px;
	border-style: solid;
	border-color: transparent;
	position: absolute;
	right: -20px;
	top: 50%;
	margin-top: -10px;
}

.center h3.primary {
	background: #0270C5;
}

.center h3.primary:after {
	border-left-color: #0270C5;
}

.center h3.danger {
	background: #E15930;
}

.center h3.danger:after {
	border-left-color: #E15930;
}

.center h3.info {
	background: #13A9A0;
}

.center h3.info:after {
	border-left-color: #13A9A0;
}

.center a {
	display: inline-block;
	text-decoration: none;
	height: 36px;
	line-height: 36px;
	font-size: 14px;
	color: #fff;
	text-align: center;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	margin-bottom: 1%;
}

.slideBox {
	width: 100%;
	height: 380px;
	overflow: hidden;
	position: relative;
}
.slideBox img {
	width: 100%;
	height: 380px;
	display: block;
	cursor:pointer;
}
.slideBox .swiper-wrapper span {
	background:rgba(0, 0, 0,0.7);
	font-size:16px;
	color:#fff;
	margin:0;
	line-height:29px;
	padding:0 20px;
	position:absolute;
	left:0;right:0;bottom:15px;
	z-index:9;
  }

