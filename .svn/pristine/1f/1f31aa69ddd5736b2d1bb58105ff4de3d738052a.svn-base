.tit {
	font-size: 32px;
	color: #333;
	font-weight: bold;
	margin: 30px 0;
	min-height: 70px;
	position: relative;
}

.tit em {
	color: #e60012;
}

.tit span {
	display: block;
	font-size: 18px;
	color: #666;
	font-weight: normal;
	position: relative;
	text-transform: uppercase;
	font-family: Arial;
	margin-top: 10px;
}

.tit span:before {
	content: "";
	display: inline-block;
	width: 14px;
	height: 14px;
	background: #e60012;
	margin-right: 10px;
}

.tit-small {
	font-size: 24px;
	min-height: auto;
}

.tit-more {
	display: inline-block;
	width: 38px;
	line-height: 16px;
	border-radius: 10px;
	background: #e60012;
	border: 1px solid #e60012;
	font-size: 12px;
	color: #fff;
	text-align: center;
	position: absolute;
	right: 0;
	bottom: 0;
	font-weight: normal;
	transition: 0.3s;
}

.tit-more:hover {
	background: #fff;
	color: #e60012;
}

.slideTxtBox {
	display: inline-block;
	vertical-align: top;
	width: 560px;
	position: relative;
	overflow: hidden;
}

.slideTxtBox .hd {
	position: absolute;
	left: 0;
	right: 20px;
	bottom: 24px;
	text-align: right;
}

.slideTxtBox .hd ul {
	font-size: 0;
}

.slideTxtBox .hd ul li {
	display: inline-block;
	width: 10px;
	height: 10px;
	border-radius: 50%;
	background: #fff;
	font-size: 0;
	margin: 0 5px;
	opacity: 0.3;
	filter: alpha(opacity = 30);
	cursor: pointer;
}

.slideTxtBox .hd ul li.on {
	opacity: 1;
	filter: alpha(opacity = 100);
}

.slideTxtBox .bd li a {
	display: block;
	height: 350px;
	position: relative;
	overflow: hidden;
}

.slideTxtBox .bd li span {
	font-size: 18px;
	color: #fff;
	line-height: 60px;
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	padding: 0 100px 0 20px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

.oneline {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.twoline {
	text-overflow: ellipsis;
	overflow: hidden;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}

.threeline {
	text-overflow: ellipsis;
	overflow: hidden;
	display: -webkit-box;
	-webkit-line-clamp: 3;
	-webkit-box-orient: vertical;
}

.info {
	padding-bottom: 10px;
	border-bottom: 1px dashed #ccc;
}

.info p {
	font-size: 14px;
	color: #666;
	line-height: 24px;
	text-indent: 2em;
}

.info .info-tit {
	display: block;
	font-size: 24px;
	color: #e60012;
	font-weight: bold;
	margin-bottom: 10px;
