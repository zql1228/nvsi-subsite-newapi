.mask {
	position: fixed;
	width: 100%;
	min-height: 100%;
	left: 0;
	top: 0;
	background: rgba(0, 0, 0, 0.5);
	z-index: -1;
	opacity: 0;
}

.mask-info {
	position: absolute;
	width: 810px;
	left: 50%;
	margin-left: -405px;
	top: 50%;
	transform: translateY(-50%);
	-webkit-transform: translateY(-50%);
	background: #fff;
	padding: 30px;
	box-sizing: border-box;
	padding-top: 0;
	max-height: 800px;
	overflow: auto;
	transform: scale(0.6);
