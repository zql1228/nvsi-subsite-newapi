/* 20191230 */
.flow {
	font-size: 0;
	counter-reset: item 0;
	padding: 32px 50px 0 50px;
}

.flow li {
	display: inline-block;
	vertical-align: middle;
	position: relative;
	font-size: 16px;
	color: #333;
	padding-top: 44px;
	text-align: center;
	width: 20%;
}

.flow li:before {
	content: counter(item);
	counter-increment: item 1;
	width: 32px;
	height: 32px;
	background-color: #C6C6C6FF;
	border-radius: 50%;
	position: absolute;
	left: 50%;
	top: 0;
	display: block;
	font-size: 20px;
	color: #fff;
	text-align: center;
	line-height: 32px;
	margin-left: -16px;
	z-index: 2;
	border: 5px solid #fff;
}

.flow li:after {
	content: "";
	display: inline-block;
	width: 100%;
	height: 2px;
	background: #F3F3F3;
	position: absolute;
	left: 50%;
	right: 0;
	top: 20px;
}

.flow li.active:before {
	background-color: #e60012;
}

.flow li.active:after {
	background: -webkit-linear-gradient(to right, #e60012, #fff);
	background: linear-gradient(to right, #e60012, #fff);
}

.flow li:last-child:after {
	display: none;
}

.header a {
