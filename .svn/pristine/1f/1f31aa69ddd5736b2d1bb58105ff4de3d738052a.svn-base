.slideBox .hd {
	height: 15px;
	overflow: hidden;
	position: absolute;
	right: 10px;
	bottom: 8px;
	z-index: 1;
}

.slideBox .hd ul {
	overflow: hidden;
	zoom: 1;
	float: left;
}

.slideBox .hd ul li {
	float: left;
	margin-right: 2px;
	width: 10px;
	height: 10px;
	line-height: 14px;
	text-align: center;
	border-radius:50%!important;
	background:#f0f0f0;
	cursor: pointer;
	opacity:0.7;
}

.slideBox .hd ul li.on {
	opacity: 1!important;
	background:#fff!important; 
	/* background: #f00; */
	/* color: #fff; */
}

.slideBox .bd {
	position: relative;
	height: 100%;
	z-index: 0;
}

.slideBox .bd li {
	zoom: 1;
	vertical-align: middle;
}

.slideBox .bd li a {
	display: block;
	position: relative;
}

.slideBox .bd img {
	width: 100%;
	height: 380px;
	display: block;
}

.slideBox .bd li a span {
	position: absolute;
	left: 0;
	bottom: 0;
	display: block;
	z-index: 100;
	width: 100%;
	height: 38px;
	background: rgba(0, 0, 0, 0.4);
	font-size: 16px;
	color: #fff;
	text-decoration: none;
	line-height: 38px;
	padding: 0 10px;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
 




.block-title {
	display: inline-block;
	font-size: 20px;
	background: #e60012;
	color: #fff;
	width: 5%;
	padding: 15px;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	text-align: center;
	line-height: 1.2;
}

.picScroll-left {
	display: inline-block;
	width: 92%;
	position: relative;
	margin-left: 35px;
	top: 20px;
}

.picScroll-left .hd {
	position: absolute;
	top: 50%;
	z-index: 100;
	width: 100%;
	margin-top: -21px;
}

.picScroll-left .hd a {
	display: inline-block;
	width: 20px;
	height: 42px;
	cursor: pointer;
	position: absolute;
	top: 0;
}

.picScroll-left .hd a.next {
	background: url(../img/left-hover.png) no-repeat center center;
	left: -10px;
}

.picScroll-left .hd a.prev {
	background: url(../img/right-hover.png) no-repeat center center;
	right: 0;
}

.picScroll-left .bd {
	padding: 10px;
}

.picScroll-left .bd .title {
	font-size: 16px;
	margin-top: 10px;
}

.picScroll-left .bd .title a {
	text-decoration: none;
	color: inherit;
}

.picScroll-left .bd ul {
	overflow: hidden;
	zoom: 1;
}

.picScroll-left .bd ul li {
	margin: 0 8px;
	float: left;
	_display: inline;
